{
  "name": "elkuku/g11n-bundle",
  "description": "Symfony G11n Bundle",
  "type": "symfony-bundle",
  "license": "WTFPL",
  "authors": [
    {
      "name": "Nikolai Plath",
      "email": "der.el.kuku@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "elkuku/g11n": "^5.0",
    "elkuku/g11n-util": "^1.1",
    "symfony/config": "^4.1",
    "symfony/dependency-injection": "^4.1",
    "symfony/http-kernel": "^4.1"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.1",
    "symfony/twig-bundle": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "ElKuKu\\G11nBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ElKuKu\\G11nBundle\\Tests\\": "Tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
